{
  "name": "detox-test",
  "version": "20.9.0",
  "private": true,
  "engines": {
    "node": ">=14.5.0"
  },
  "scripts": {
    "lint": "eslint .",
    "test:types": "tsc -p types/tsconfig.json",
    "test": "npm run lint && npm run test:types",
    "postinstall": "node scripts/postinstall.js",
    "start": "react-native start",
    "packager": "react-native start",
    "detox-server": "detox run-server",
    "e2e:ios": "detox test -c ios.sim.release",
    "e2e:android": "detox test -c android.emu.release",
    "e2e:android:genycloud": "detox test -c android.genycloud.release",
    "e2e:android:debug": "detox test -c android.emu.debug",
    "e2e-unhappy:ios": "scripts/ci_unhappy.sh ios",
    "react": "18.1.0",
    "react-native": "0.70.7",
    "react-native-gradle-plugin": "^0.0.7",
    "react-native-launch-arguments": "^3.1.0",
    "react-native-webview": "^11.18.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/node": "^14.18.33",
    "@types/react": "18.0.0",
    "@types/react-native": "0.69.7",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "cross-env": "^7.0.3",
    "detox": "^20.9.0",
    "eslint": "^7.32.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "execa": "^5.1.1",
    "express": "^4.15.3",
    "glob": "^7.2.0",
    "jest": "^29.2.0",
    "jest-allure2-reporter": "^1.2.0",
    "jest-junit": "^10.0.0",
    "lodash": "^4.14.1",
    "metro-react-native-babel-preset": "^0.70.3",
    "nyc": "^15.1.0",
    "p-iteration": "^1.1.8",
    "pngjs": "^3.4.0",
    "react-native-codegen": "^0.0.8",
    "typescript": "^4.1.3"
  },
  "jest-junit": {
    "suiteName": "Detox E2E tests",
    "outputName": "e2e-junit.xml",
    "ancestorSeparator": " > ",
    "classNameTemplate": "E2E Results.{classname}",
    "titleTemplate": "{classname} > {title}"
  },
  "detox": {
    "extends": "./e2e/detox.config"
  }
}
